@model IEnumerable<AirUdC.GUI.Models.Parameters.MultimediaTypeModel>

    @{
    ViewBag.Title = "Tipo multimedia";
    }

    <div class="container">
        <h2>Tipo de Multimedia</h2>

        <div class="row">
            <div class="col-md-12">
                <p>
                    @Html.ActionLink("Agregar Nuevo", "Create", null, new { @class = "btn btn-primary" })
                </p>
            </div>
        </div>

        <div class="row">
            <div class="col-md-12">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>@Html.DisplayNameFor(model => model.MultimediaTypeName)</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model) {
                        <tr>
                            <td>@Html.DisplayFor(modelItem => item.MultimediaTypeName)</td>
                            <td>
                                @Html.ActionLink("Editar", "Edit", new { id = item.MultimediaTypeId }, new { @class = "btn btn-info" }) |
                                @Html.ActionLink("Detalles", "Details", new { id = item.MultimediaTypeId }, new { @class = "btn btn-success" }) |
                                @Html.ActionLink("Eliminar", "Delete", new { id = item.MultimediaTypeId }, new { @class = "btn btn-danger" })
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
